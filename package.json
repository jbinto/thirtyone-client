{
  "name": "thirtyone-client",
  "version": "0.0.1",
  "description": "Web client for web-based 31 card game.",
  "main": "index.js",
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "test": "NODE_ENV=test node_modules/.bin/karma start karma.config.js",
    "test:watch": "npm run test -- --watch",
    "test:mocha": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:mocha:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbinto/thirtyone-client.git"
  },
  "author": "Jesse Buchanan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbinto/thirtyone-client/issues"
  },
  "homepage": "https://github.com/jbinto/thirtyone-client#readme",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^1.3.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-hmr": "^1.0.1",
